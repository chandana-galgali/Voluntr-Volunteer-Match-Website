{
  "ci": {
    "collect": {
      "startServerCommand": "npx serve -s build -l 4173",
      "startServerReadyPattern": "Local:.*4173|http://localhost:4173|Accepting connections|listening on.*4173",
      "startServerReadyTimeout": 60000,
      "settings": {
        "preset": "desktop",
        "throttlingMethod": "provided",
        "blockedUrlPatterns": ["*googletagmanager.com*"]
      },
      "url": [
        "http://localhost:4173/",
        "http://localhost:4173/volunteer-list",
        "http://localhost:4173/volunteer-form"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.85 }],
        "categories:accessibility": ["warn", { "minScore": 0.90 }],
        "categories:best-practices": ["warn", { "minScore": 0.90 }],
        "categories:seo": ["warn", { "minScore": 0.90 }]
      }
    }
  }
}